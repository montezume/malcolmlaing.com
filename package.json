{
  "name": "malcolmlaing.com",
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.0.1",
  "author": "Malcolm Laing <malcolmclaing@gmail.com>",
  "dependencies": {
    "gatsby": "2.0.98",
    "gatsby-image": "2.0.28",
    "gatsby-plugin-manifest": "2.0.13",
    "gatsby-plugin-offline": "2.0.21",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-sharp": "2.0.18",
    "gatsby-source-filesystem": "2.0.17",
    "gatsby-transformer-sharp": "2.1.12",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0"
  },
  "keywords": [
    "gatsby",
    "front end developer",
    "react"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint \"./**/*.js\"",
    "start": "npm run develop",
    "format:md": "prettier --write --parser markdown '**/*.md'",
    "format:js": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "@babel/parser": "7.3.1",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "prettier": "1.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
